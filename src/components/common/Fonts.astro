---
import { AstroFont } from "astro-font";
---
<AstroFont
      config={[
        {
            src: [
            {
              style: 'normal',
              weight: '400, 600, 700, 900',
              path: './public/fonts/Raleway-VariableFont.woff2'
            },
          ],
          name: "Raleway",
          preload: true,
          display: "swap",
          selector: "body",
          fallback: "sans-serif",
        },
      ]}
    />
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin="anonymous" />


