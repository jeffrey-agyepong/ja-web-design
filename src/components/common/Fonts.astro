---
import { AstroFont } from "astro-font";
---
<AstroFont
      config={[
        {
            src: [
            {
              style: 'normal',
              weight: '400, 500, 600, 700, 900',
              path: './fonts/Raleway-VariableFont.woff2'
            },
          ],
          name: "Raleway",
          googleFontsURL: 'https://fonts.googleapis.com/css2?family=Raleway:wght@400;700&display=swap',
          preload: true,
          display: "swap",
          selector: "body",
          fallback: 'sans-serif', // Corrected fallback syntax
        },
      ]}
    />
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin="anonymous" />


